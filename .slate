config defaultToCurrentScreen true
config nudgePercentOf screenSize
config resizePercentOf screenSize

# Push Bindings
bind right:ctrl;cmd  move (screenSizeX-165)/2;0 (screenSizeX-165)/2;screenSizeY
bind left:ctrl;cmd   push left  bar-resize:(screenSizeX-165)/2
bind up:ctrl;cmd     push left  bar-resize:screenSizeX-165
